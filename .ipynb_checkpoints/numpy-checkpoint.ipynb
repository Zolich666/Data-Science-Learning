   "source": [
    "import numpy as np\n",
    "# 列表转矩阵\n",
    "array = np.array([\n",
    "    [1,2,3],\n",
    "    [4,5,6]\n",
    "])\n",
    "print(array)\n",
    "\n",
    "# 打印矩阵维度\n",
    "print('dim:',array.ndim)\n",
    "\n",
    "# 打印行数和列数\n",
    "print('shape:',array.shape)\n",
    "\n",
    "# 打印元素个数\n",
    "print('size:',array.size)"
   ]
